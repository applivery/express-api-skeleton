{
  "name": "express-api-skeleton",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "lint": "eslint .",
    "start": "node index.js",
    "serve": "DEBUG=AP:* NODE_ENV=development nodemon index.js",
    "test": "NODE_ENV=test jest --verbose",
    "test-dev": "NODE_ENV=test jest --watch --verbose",
    "test-coverage": "NODE_ENV=test jest --coverage",
    "deploy": "yarn now-deploy && yarn now-alias && yarn now-rm-old",
    "now-deploy": "now --npm -n express-api-skeleton --regions bru1",
    "now-alias": "now alias",
    "now-rm-old": "now rm express-api-skeleton --safe --yes"
  },
  "engines": {
    "node": ">=8",
    "yarn": "*"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "debug": "^4.1.0",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "express-deliver": "^1.0.0",
    "express-mung": "^0.5.1",
    "express-validation": "^1.0.2",
    "helmet": "^3.15.0",
    "http-status": "^1.2.0",
    "joi": "^14.0.4",
    "jwt-simple": "^0.5.5",
    "method-override": "^3.0.0",
    "moment-timezone": "^0.5.23",
    "mongoose": "^5.3.9",
    "mongoose-paginate-v2": "^1.0.12",
    "mongoose-timestamp": "^0.6.0",
    "mongoose-to-swagger": "^0.0.4",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "swagger-jsdoc": "^3.2.3",
    "swagger-ui-express": "^4.0.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "jest": "^23.6.0",
    "mockingoose": "^2.11.0",
    "nodemon": "^1.18.6",
    "supertest": "^3.3.0"
  }
}
